{
    "name": "tr-lang",
    "displayName": "tr-lang",
    "description": "tr-lang language support for Visual Studio Code",
    "publisher": "kaiserthe13th",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.63.0"
    },
    "homepage": "https://github.com/kaiserthe13th/tr-lang",
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "icon": "img/icon.png",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/kaiserthe13th/tr-lang",
        "directory": "highlight/editors/vscode/tr-lang"
    },
    "contributes": {
        "languages": [{
            "id": "tr-lang",
            "aliases": ["tr-lang", "trl"],
            "extensions": [".trl", ".tr-lang"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tr-lang",
            "scopeName": "source.tr-lang",
            "path": "./syntaxes/tr-lang.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "tr-lang",
                "path": "./snippets/tr-lang.json"
            }
        ]
    }
}
